{
    "name": "perf-analytics-dashboard",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "test": "jest",
        "watch-test": "jest --watchAll",
        "postinstall-heroku": "npm run build",
        "wp:server": "webpack --env 'development' --config src/config/webpack.server.js",
        "wp:client": "webpack --env 'development' --config src/config/webpack.client.js",
        "dev": "npm run wp:client && npm run wp:server && npm-run-all -p webpack:*",
        "build": "npm run clean && webpack --env 'production' --config src/config/webpack.client.js && webpack --env 'production' --config src/config/webpack.server.js",
        "start": "node build/server.js",
        "clean": "rimraf build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
